{
  "name": "michaelravedoni/laravel-value-objects",
  "description": "A base package for creating value objects in Laravel, with Artisan command and generic Eloquent cast.",
  "version": "1.0.0",
  "keywords": [
    "laravel",
    "value objects",
    "eloquent",
    "casting"
  ],
  "homepage": "https://github.com/michaelravedoni/laravel-value-objects",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Ravedoni",
      "homepage": "https://michaelravedoni.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.2 || ^8.3 || ^8.4",
    "illuminate/contracts": "^9.0 || ^10.0 || ^11.0 || ^12.0",
    "illuminate/support": "^9.0 || ^10.0 || ^11.0 || ^12.0",
    "illuminate/database": "^9.0 || ^10.0 || ^11.0 || ^12.0",
    "illuminate/console": "^9.0 || ^10.0 || ^11.0 || ^12.0"
  },
  "require-dev": {
    "laravel/pint": "^1.23",
    "orchestra/testbench": "^9.0",
    "pestphp/pest": "^2.0",
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "MichaelRavedoni\\LaravelValueObjects\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MichaelRavedoni\\LaravelValueObjects\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-pest": "vendor/bin/pest"
  },
  "extra": {
    "laravel": {
      "providers": [
        "MichaelRavedoni\\LaravelValueObjects\\LaravelValueObjectsServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
